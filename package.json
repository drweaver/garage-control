{
  "name": "garage-control",
  "version": "1.0.0",
  "description": "Garage Control using `socket.io` with a static `express` server.",
  "main": "garage-control.js",
  "repository": "https://github.com/drweaver/garage-control",
  "author": "Shane Weaver",
  "keywords": [
    "garage",
    "door",
    "opener",
    "pushbullet",
    "raspberry pi",
    "piface",
    "pfio"
  ],
  "dependencies": {
    "express": "~4.14.0",
    "compression": "~1.6.2",
    "socket.io": "~1.4.8",
    "geodist": "~0.2.1",
    "pushbullet": "~1.4.2",
    "async": "~1.5.0",
    "http-proxy": "~1.14.0",
    "mqtt": "~2.1.3",
    "amqplib": "~0.5.1"
  },
  "optionalDependencies": {
    "piface-node": "~0.0.6"
  },
  "scripts": {
    "start": "nohup $(which node) $(pwd)/garage-control.js > /dev/null &",
    "stop":  "pkill -xfe $(which node)\\ $(pwd)/garage-control.js || true"
  }
}
